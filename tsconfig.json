{
    "include": ["src"],
    "compilerOptions": {
      "strict": true,
      "module": "esnext",
      "esModuleInterop": true,
      "lib": ["es2016", "dom", "dom.iterable"],
      "target": "ES2020",
      "moduleResolution": "node",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "rootDir": "src",
      "outDir": "out"
    }
  }
